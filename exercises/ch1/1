#include <stdio.h>

#define IN 1
#define out 0
#define MAXARRAYSIZE 64
#define MAXCHARS 100

/* Print a histogram of the different frequencies of different characterss input */


int main()
{

    int c, i, nothers, nwhites;
    int ndigit[10];

    int alphacaps[26];
    int alphalowers[26];

    nwhites = nothers = 0;

    /* Initialise digit count array */
    for (i = 0; i <=  10; ++i) {
	ndigit[i] = 0;
    }

    for (i = 0; i <= 26; ++i) {
	alphacaps[i] = 0;
	alphalowers[i] = 0;
    }

    while ((c = getchar()) != EOF) {
	if ( c >= '0' && c <= '9') {
		++ndigit[c-'0'];
	} else if (c >= 'a' && c <= 'z') {
		++alphalowers[c-'a'];
	} else if (c >= 'A' && c <= 'Z') {
		++alphacaps[c-'A'];
	} else if (c == ' ' || c == '\n' || c == '\t') {
		++nwhites;
	} else
		++nothers;
    }

    printf("Digits = ");
    for (i = 0; i < 10; ++i)
	printf(" %d", ndigit[i]);

    printf("\n");

    printf("Lower case characters = ");
    for (i = 0; i < 26; ++i)
	printf(" %d", alphalowers[i]);

    printf("\n");
    printf("Upper case characters = ");
    for (i = 0; i < 26; ++i)
	printf(" %d", alphacaps[i]);

    printf("\nWhite space = %d,\nOther = %d\n", nwhites, nothers);

}
